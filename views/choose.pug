extends layout

block content
  h1= title

  - var image_names = ["bahubali.jpeg", "images/books.jpg", "images/KGF.jpeg", "images/RRR.jpeg", "images/Wings of fire.jpg"]

  - for(let i = 0; i<3; i++)
    - let index = Math.floor(Math.random()*5)
    - let images = image_names[index]
    img(src=`/images/${images}`, alt=`${images}`, height="300", width="300")